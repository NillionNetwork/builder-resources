{
  "name": "with-src",
  "displayName": "With src",
  "version": "0.0.1",
  "description": "Plasmo + Nillion JS Client",
  "author": "wwwehr",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@nillion/nillion-client-js-browser": "file:/path/to/nillion/sdk/nillion-client-browser-npm.tar.gz",
    "plasmo": "0.84.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'none';"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "@nillion/nillion-client-js-browser/nillion_client_js_browser.js"
        ],
        "matches": [
          "file://*"
        ]
      }
    ]
  }
}
